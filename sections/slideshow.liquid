{{ 'section-image-banner.css' | asset_url | stylesheet_tag }}
{{ 'swiper.css' | asset_url | stylesheet_tag }}
{{ 'section-slideshow.css' | asset_url | stylesheet_tag }}

<script src="{{ 'swiper.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'section-slideshow.js' | asset_url }}" defer="defer"></script>

{% assign pagination_style = settings.media_pagination_style %}

{% assign slides_arrows = section.settings.show_arrows %}

{%- style -%}

.section-{{ section.id }} .banner-slideshow-text-container {
  padding-top: {{ section.settings.top_padding | times: 0.8 }}px;
  padding-bottom: {{ section.settings.bottom_padding | times: 0.8 }}px;
}

.section-{{ section.id }} .images-container, .section-{{ section.id }} .banner-slideshow-text-container {
  padding-top: {{ section.settings.section_top_padding | times: 0.8 }}px;
  padding-bottom: {{ section.settings.section_bottom_padding | times: 0.8 }}px;  
}

@media only screen and (min-width: 750px) {
  .section-{{ section.id }} .images-container, .section-{{ section.id }} .banner-slideshow-text-container {
    padding-top: {{ section.settings.section_top_padding }}px;
    padding-bottom: {{ section.settings.section_bottom_padding }}px;  
  }
  .section-{{ section.id }} .banner-slideshow-text-container {
    padding-top: {{ section.settings.top_padding }}px;
    padding-bottom: {{ section.settings.bottom_padding }}px;
  }  
}
  
{%- if section.settings.overlay_level < 10 -%}
.section-{{ section.id }} .banner-overlay {
  background: rgba(0,0,0,0.05);
}
{%- else -%}
.section-{{ section.id }} .banner-overlay {
  background: rgba(0,0,0,0.{{ section.settings.overlay_level }});
}
{%- endif -%}

{% if section.settings.txt_alignment == 'left' %}
.section-{{ section.id }} .banner-full-bleed-text {
  text-align: left;
}
{% endif %}
  
{% if section.settings.txt_alignment == 'center' %}
.section-{{ section.id }} .banner-full-bleed-text {
  text-align: center;
}
.section-{{ section.id }} .banner-full-bleed-text > div {
  text-align: center;
  margin: 0 auto;
}
  
.section-{{ section.id }} .banner-full-bleed-text img {
  text-align: center;
  margin: 0 auto;
}

{% endif %}

{% if section.settings.txt_position == 'middle' %}
.section-{{ section.id }} .section-inner {
  display: flex;
  align-items: center;
}
{% endif %}

{% if section.settings.txt_position == 'top' %}
.section-{{ section.id }} .section-inner {
  display: flex;
  align-items: flex-start;
}
{% endif %}

{% if section.settings.txt_position == 'bottom' %}
.section-{{ section.id }} .section-inner {
  display: flex;
  align-items: flex-end;
}
{% endif %}

@media only screen and (min-width: 750px) {
  .section-{{ section.id }} .banner-slideshow-text-container {
    max-width: {{ section.settings.txt_width }}px;
  }
  .section-{{ section.id }} .slideshow-text-wrapper {
    max-width: {{ section.settings.txt_width }}px;
  }
}

@media only screen and (min-width: 1025px) {
  .section-{{ section.id }} .banner-slideshow-text-container {
    max-width: {{ section.settings.txt_width }}px;
  }
  .section-{{ section.id }} .slideshow-text-wrapper {
    max-width: {{ section.settings.txt_width }}px;
  }
}
  
{%- endstyle -%} 

<div class="section-container slideshow-section banner-full-bleed section-{{ section.id }} {{ section.settings.banner_size }} banner-margins-{{ section.settings.section_margin_width }}">
<div class="section-inner {{ section.settings.section_margin_width }} slideshow-images-{{ section.settings.media_scaling }} slideshow-arrows-{{ slides_arrows }} color-scheme-{{ section.settings.text_color }} color-scheme-{{ section.settings.bg_color }}">

{% if section.settings.slideshow_type == 'single_message' %}
<div class="banner-slideshow-text-container page-width text-position--{{ section.settings.txt_position }} align--{{ section.settings.txt_alignment }}"> 
  <div class="slideshow-text-wrapper">
  {% if section.settings.section_label != blank %}<span class="section-label">{{ section.settings.section_label }}</span>{% endif %}
  {% if section.settings.heading_tag == 'h1' %}
    <h1 class="{{ section.settings.heading_size }}">{{ section.settings.heading }}</h1>
  {% else %}
    <h2 class="{{ section.settings.heading_size }}">{{ section.settings.heading }}</h2>
  {% endif %}

  {% if section.settings.subheading != blank %}
  <div class="{{ section.settings.text_size }}">
  {{ section.settings.subheading }}
  </div>
  {% endif %}

  {% unless section.settings.btn_one_txt == blank and section.settings.btn_two_txt %}
  <div class="full-bleed-banner-btn buttons-layout-{{ section.settings.btn_layout }}">
  {% if section.settings.btn_one_txt != blank %}
  <a href="{{ section.settings.btn_one_link }}" class="{% unless section.settings.btn_one_style == 'link--underlined' %}button{% endunless %} {{ section.settings.btn_one_style }}">{{ section.settings.btn_one_txt }}</a>
  {% endif %}
  {% if section.settings.btn_two_txt != blank %}
  <a href="{{ section.settings.btn_two_link }}" class="{% unless section.settings.btn_one_style == 'link--underlined' %}button{% endunless %} {{ section.settings.btn_two_style }}">{{ section.settings.btn_two_txt }}</a>
  {% endif %}
  </div>
  {% endunless %}
  </div>
</div>
{% endif %}

<div class="images-container slider-align-{{ section.settings.txt_alignment }} pagination-style-{{ pagination_style }} arrows-style-{{ section.settings.arrow_style }}"> 
<div class="swiper mySwiper" 
     data-speed="{{ section.settings.slider_speed }}" 
     data-interval="{{ section.settings.slider_autoplay_interval | times: 1000 }}" 
     data-autoplay="{{ section.settings.slider_autoplay }}" 
     data-effect="{{ section.settings.slider_effect }}">
    {% unless section.blocks.size < 2 %}
    <div class="swiper-button-prev">{% render 'icon-caret' %}</div>
    <div class="swiper-button-next">{% render 'icon-caret' %}</div>
    {% endunless %}
    <div class="swiper-pagination"></div>
    <div class="swiper-wrapper">
    {% for block in section.blocks %}
    {% case block.type %}

    {% when 'slide' %}
    <div class="swiper-slide swiper-slide-fw" data-index="{{ forloop.index }}" {{ block.shopify_attributes }}>
              {% if section.settings.slideshow_type == 'multi_message' %}
              <div class="slideshow-slide-text sst-{{ section.settings.txt_position }} page-width {{ section.settings.txt_alignment }}">
              <div class="slideshow-text-wrapper">
              {% if block.settings.caption != blank %}<span class="section-label">{{ block.settings.caption }}</span>{% endif %}
              {% if block.settings.heading != blank %}<h2 class="{{ section.settings.heading_size }}">{{ block.settings.heading }}</h2>{% endif %}
              {% if block.settings.subheading != blank %}<span class="{{ section.settings.text_size }}">{{ block.settings.subheading }}</span>{% endif %}
              {% if block.settings.btn_block_txt != blank %}
              <div class="slideshow-slide-buttons button-color-scheme--{{ section.settings.btn_color_scheme }}">
              <a href="{{ block.settings.btn_block_url }}" class="{% unless block.settings.btn_block_style == 'link--underlined' %}button{% endunless %} {{ block.settings.btn_block_style }}">{{ block.settings.btn_block_txt }}</a>
              </div>
              {% endif %}
              </div>
              </div>
              {% endif %}
    <div class="bg-pane banner-overlay"></div>
    <div class="swiper-slide--image{% if block.settings.image_mobile %} swipe-slide--image-with-mobile-image{% endif %}{% if section.settings.media_scaling == 'cover' %} image-animation-{{ block.settings.image_animation }}{% endif %}">
      {% if block.settings.image %}
      {% if forloop.index == 0 %}
      {{ block.settings.image | image_url: width: 2600 | image_tag: class: 'desktop-image' }}
      {% else %}
      {{ block.settings.image | image_url: width: 2600 | image_tag: class: 'desktop-image', loading: 'lazy' }}
      {% endif %}
      {% endif %}
      {% if block.settings.image_mobile %}
      {% if forloop.index == 0 %}
      {{ block.settings.image_mobile | image_url: width: 1600 | image_tag: class: 'mobile-image' }}
      {% else %}
      {{ block.settings.image_mobile | image_url: width: 1600 | image_tag: class: 'mobile-image', loading: 'lazy' }}
      {% endif %}
      {% endif %}
      {% if block.settings.image == blank and block.settings.image_mobile == blank %}
      {% render 'media-placeholder-lifestyle', style: 7 %}
      {% endif %}
    </div>
    </div>
    {% when 'video_slide' %}
    <div class="swiper-slide swiper-slide-fw" data-index="{{ forloop.index }}" {{ block.shopify_attributes }}>
              {% if section.settings.slideshow_type == 'multi_message' %}
              <div class="slideshow-slide-text sst-{{ section.settings.txt_position }} page-width {{ section.settings.txt_alignment }}">
              <div class="slideshow-text-wrapper">
              {% if block.settings.caption != blank %}<span class="section-label">{{ block.settings.caption }}</span>{% endif %}
              {% if block.settings.heading != blank %}<h2 class="{{ section.settings.heading_size }}">{{ block.settings.heading }}</h2>{% endif %}
              {% if block.settings.subheading != blank %}<span class="{{ section.settings.text_size }}">{{ block.settings.subheading }}</span>{% endif %}
              {% if block.settings.btn_block_txt != blank %}
              <div class="slideshow-slide-buttons button-color-scheme--{{ section.settings.btn_color_scheme }}">
              <a href="{{ block.settings.btn_block_url }}" class="{% unless block.settings.btn_block_style == 'link--underlined' %}button{% endunless %} {{ block.settings.btn_block_style }}">{{ block.settings.btn_block_txt }}</a>
              </div>
              {% endif %}
              </div>
              </div>
              {% endif %}
    <div class="bg-pane banner-overlay"></div>
    <div class="swiper-slide--video autoplaying-video">
    {% if block.settings.video_image %}
    <div class="slideshow-video-poster {{ block.id }}-video-backup-poster">
      {{ block.settings.video_image | image_url: width: 1080 | image_tag: loading: 'lazy' }}
    </div>
    {% endif %}
    {% if block.settings.video_mobile != null %}
    {% assign video_id = 'vid-mobile-' | append: block.id %}
    <div class="slideshow-video-player slideshow-video-mobile">
      {{
        block.settings.video_mobile
        | video_tag:
          id: video_id,
          image_size: '1080x',
          autoplay: false,
          loop: true,
          controls: false,
          muted: true
      }} 
    </div>
    {% endif %}
    {% if block.settings.video != null %}
    {% assign video_id = 'vid-desktop-' | append: block.id %}
    <div class="slideshow-video-player{% if block.settings.video_mobile != null %} slideshow-video-desktop{% endif %}">
      {{
        block.settings.video
        | video_tag:
          id: video_id,
          image_size: '1800x',
          autoplay: false,
          loop: true,
          controls: false,
          muted: true
      }} 
    </div>
    {% endif %}
    {% if block.settings.video == blank and block.settings.image == blank %}
    {% render 'media-placeholder-lifestyle', style: 7 %}
    {% endif %}
    </div>
    </div>

    <script>
      document.addEventListener('DOMContentLoaded', function () {
        var videoElement = document.getElementById('vid-mobile-{{ block.id }}');
        var posterDiv = document.querySelector('.{{ block.id }}-video-backup-poster');
    
        if (videoElement) {
          var videoPromise = videoElement.play();
          if (videoPromise) {
            videoPromise
              .then(function () {
                // Function
              })
              .catch(function (error) {
                if (error.name === 'NotAllowedError') {
                  // Handle "NotAllowedError"
                  posterDiv.classList.add('poster-visible');
                }
              });
          }
        }
      });
    </script>
      
    {% endcase %}

    {% endfor %}
  </div>
</div>
</div>

</div>
</div>

  
