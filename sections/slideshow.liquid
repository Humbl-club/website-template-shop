      ]
    },
    {
      "type": "slide",
      "name": "t:sections.slideshow.blocks.slide.name",
      "limit": 8,
      "settings": [
    {
	"type": "image_picker",
	"id": "image",
	"label": "t:sections.all.image"
	},
    {
	"type": "image_picker",
	"id": "image_mobile",
	"label": "t:sections.slideshow.settings.mobile_image.label",
    "info": "t:sections.slideshow.settings.mobile_image.info"
	},
    {
      "type": "select",
      "id": "image_animation",
      "label": "t:sections.all.animation.animation_effect",
      "options": [
        {
          "value": "none",
          "label": "t:sections.all.none"
        },
        {
          "value": "zoom-in",
          "label": "t:sections.all.animation.zoom_in"
        },
        {
          "value": "zoom-out",
          "label": "t:sections.all.animation.zoom_out"
        },
        {
          "value": "pan-left",
          "label": "t:sections.all.animation.pan_left"
        },
        {
          "value": "pan-right",
          "label": "t:sections.all.animation.pan_right"
        },
        {
          "value": "pan-up",
          "label": "t:sections.all.animation.pan_up"
        },
        {
          "value": "pan-down",
          "label": "t:sections.all.animation.pan_down"
        }
		],
      "default": "none"
    },
    {
      "type": "header",
      "content": "Content"
    },
    {
	"type": "text",
	"id": "caption",
	"label": "t:sections.all.caption"
	},
    {
	"type": "text",
	"id": "heading",
	"label": "t:sections.all.heading"
	},
    {
	"type": "richtext",
	"id": "subheading",
	"label": "t:sections.all.sub_heading"
	},
    {
      "type": "header",
      "content": "t:sections.all.buttons.button_header"
    },
    {
	"type": "text",
	"id": "btn_block_txt",
	"label": "t:sections.all.buttons.button_label"
	},
    {
	"type": "url",
	"id": "btn_block_url",
	"label": "t:sections.all.buttons.button_link"
	},
    {
      "type": "select",
      "id": "btn_block_style",
      "label": "t:sections.all.buttons.button_style",
      "options": [
        {
          "value": "button--solid",
          "label": "t:sections.all.buttons.button_style_solid"
        },
        {
          "value": "button--outline",
          "label": "t:sections.all.buttons.button_style_outline"
        },
        {
          "value": "link--underlined",
          "label": "t:sections.all.buttons.button_style_link"
        }
		],
      "default": "button--solid"
    }
      ]
    }
  ],
    "settings": [
    {
      "type": "select",
      "id": "banner_size",
      "label": "t:sections.slideshow.settings.slideshow_size.label",
      "options": [
        {
          "value": "banner-style-fullscreen",
          "label": "t:sections.all.size.fullscreen"
        },
        {
          "value": "banner-style-large",
          "label": "t:sections.all.size.large"
        },
        {
          "value": "banner-style-medium",
          "label": "t:sections.all.size.medium"
        },
        {
          "value": "banner-style-slim",
          "label": "t:sections.all.size.small"
        }
		],
      "default": "banner-style-fullscreen"
    },
    {
      "type": "select",
      "id": "slideshow_type",
      "label": "t:sections.slideshow.settings.slideshow_type.label",
      "options": [
        {
          "value": "multi_message",
          "label": "t:sections.slideshow.settings.slideshow_type.options__1.label"
        },
        {
          "value": "single_message",
          "label": "t:sections.slideshow.settings.slideshow_type.options__2.label"
        }
		],
      "default": "multi_message"
    },
    {
      "type": "select",
      "id": "section_margin_width",
      "label": "t:sections.all.margin",
