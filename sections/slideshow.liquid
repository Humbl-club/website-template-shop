    {
	"type": "richtext",
	"id": "subheading",
	"label": "t:sections.all.sub_heading"
	},
    {
      "type": "select",
      "id": "heading_size",
      "label": "t:sections.all.heading_size.label",
      "options": [
        {
          "value": "h2",
          "label": "t:sections.all.size.small"
        },
        {
          "value": "h1",
          "label": "t:sections.all.size.medium"
        },
        {
          "value": "h0",
          "label": "t:sections.all.size.large"
        },
        {
          "value": "hxl",
          "label": "t:sections.all.size.xl"
        }
		],
      "default": "h1"
    },
    {
      "type": "select",
      "id": "text_size",
      "label": "t:sections.slideshow.settings.text_size.label",
      "options": [
        {
          "value": "text-body",
          "label": "t:sections.all.size.normal"
        },
        {
          "value": "text-large",
          "label": "t:sections.all.size.large"
        },
        {
