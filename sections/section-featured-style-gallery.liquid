{%- style -%}
.section-{{ section.id }} .section-inner {
  padding-top: {{ section.settings.padding_top | times: 0.8 | round: 0 }}px;
  padding-bottom: {{ section.settings.padding_bottom | times: 0.8 | round: 0 }}px;
}
  
@media only screen and (min-width: 750px) {
  .section-{{ section.id }} .section-inner {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
  }  
}

@media only screen and (min-width: 1025px) { 
  .section-{{ section.id }}:has(.slider-arrows-position-top-right):has(.section-header.text-align-right) .section-header-std-padding {
    margin-bottom: {{ settings.section_header_spacing | at_least: 70 }}px;
  }
} 
{%- endstyle -%}

{% assign thumbnail_size = 720 %}
{% if section.settings.grid_layout_desktop < 4 %}
{% assign thumbnail_size = 900 %}
{% endif %}
{% if section.settings.grid_layout_desktop < 3 %}
{% assign thumbnail_size = 1280 %}
{% endif %}
  
<div class="section-{{ section.id }} gallery-aso gallery-aso-ratio-{{ section.settings.image_ratio }} color-scheme-{{ section.settings.text_color }} color-scheme-{{ section.settings.bg_color }}">

{% if section.settings.top_border %}
<hr class="section-border section-border-color-{{ section.settings.border_color }} section-border-width-{{ section.settings.border_width }}">
{% endif %}
  
<div class="section-inner">
  
{% unless section.settings.section_label == blank and section.settings.heading == blank and section.settings.sub_heading == blank %}
<div class="section-header section-header-std-padding page-width text-align-{{ section.settings.content_alignment }}">  
{% unless section.settings.section_label == blank %}<div class="section-label">{{ section.settings.section_label }}</div>{% endunless %}
{% unless section.settings.heading == blank %}<h2 class="{{ section.settings.heading_size }}">{{ section.settings.heading }}</h2>{% endunless %}
{% unless section.settings.sub_heading == blank %}<div class="section-header-paragraph">{{ section.settings.sub_heading }}</div>{% endunless %}
</div>
{% endunless %}

<div class="aso-grid-wrapper{% if section.settings.thumbnails_layout == 'carousel' %} slider-outer-wrapper slider-arrows-position-{{ settings.carousel_arrow_position }}{% endif %}{% if section.blocks.size < section.settings.grid_layout_desktop and section.settings.content_alignment == 'center' %} layout-center-desktop{% endif %}{% if section.settings.thumbnails_layout == 'grid' %} page-width{% endif %}">
  
<ul id="grid-{{ section.id }}"{% if section.settings.thumbnails_layout == 'grid' %} class="aso-static-grid static-grid--columns-mobile-{{ section.settings.grid_layout_mobile }} static-grid--columns-tablet-{{ section.settings.grid_layout_tablet }} static-grid--columns-desktop-{{ section.settings.grid_layout_desktop }}"{% else %} class="aso-slider grid slider-element slider-grid slider-grid--columns-mobile-{{ section.settings.grid_layout_mobile }} slider-grid--columns-tablet-{{ section.settings.grid_layout_tablet }} slider-grid--columns-desktop-{{ section.settings.grid_layout_desktop }}"{% endif %}>
{% for block in section.blocks %}
  {% assign accessibility_title = block.settings.preview_title %}
  {% if block.settings.preview_title == blank %}
  {% assign accessibility_title = 'accessibility.open' | t %}
  {% endif %}
  <li class="grid__item gallery-card-aso" {{ block.shopify_attributes }}>
  <button class="modal-opener-link" data-index="{{ forloop.index }}" aria-haspopup="dialog" aria-label="{{ accessibility_title }}">
  <div class="gallery-card-aso-image-wrapper">
  <div class="gallery-card-aso-image{% if section.settings.show_hotspots %} hotspot-color-scheme-{{ section.settings.hotspot_color }}{% endif %}">
      {% if block.settings.product_one != blank and section.settings.show_hotspots %}
        <style>
        .hotspot-{{ block.id }}-one {
          left: {{ block.settings.product_one_pos_x }}% !important;
          top: {{ block.settings.product_one_pos_y }}% !important;
        }
        </style>
        <div class="phs-hotspot hotspot-{{ block.id }}-one"></div>
      {% endif %}
      {% if block.settings.product_two != blank and section.settings.show_hotspots %}
        <style>
        .hotspot-{{ block.id }}-two {
          left: {{ block.settings.product_two_pos_x }}% !important;
          top: {{ block.settings.product_two_pos_y }}% !important;
        }
        </style>
        <div class="phs-hotspot hotspot-{{ block.id }}-two"></div>
      {% endif %}
      {% if block.settings.product_three != blank and section.settings.show_hotspots %}
        <style>
        .hotspot-{{ block.id }}-three {
          left: {{ block.settings.product_three_pos_x }}% !important;
          top: {{ block.settings.product_three_pos_y }}% !important;
        }
        </style>
        <div class="phs-hotspot hotspot-{{ block.id }}-three"></div>
      {% endif %}
  {% if block.settings.image %}
  {{ block.settings.image | image_url: width: thumbnail_size | image_tag: loading: 'lazy' }}
  {% else %}
  {% render 'media-placeholder-lifestyle', style: 8 %}
  {% endif %}
  </div>
  </div>
   {% if section.settings.show_thumbnail_title and block.settings.preview_title != blank %}<div class="gallery-card-aso-text preview-title-position-{{ section.settings.preview_title_position }} text-align-{{ section.settings.content_alignment }}"><h3>{{ block.settings.preview_title }}</h3></div>{% endif %}
  </button>
  </li>
{% endfor %}
</ul>

{% if section.blocks.size > section.settings.grid_layout_desktop and section.settings.thumbnails_layout == 'carousel' %}
<div class="slider-arrows arrow-style-{{ section.settings.arrow_style }}">
  <button class="slider-arrow-left" aria-label="{{ 'accessibility.left' | t }}">{% render 'icon-caret' %}</button>
  <button class="slider-arrow-right" aria-label="{{ 'accessibility.right' | t }}">{% render 'icon-caret' %}</button>
</div>
{% endif %}

{% render 'scrolltrack' %}
  
</div>

{% for block in section.blocks %}
<div class="aso-modal" id="{{ section.id }}-modal-{{ forloop.index }}" role="dialog" aria-modal="true" tabindex="-1">
<div class="aso-modal-inner{% if settings.popup_corner_radius > 24 %} aso-modal-inner--large-corner-radius{% endif %}">
  <button class="modal-close" aria-label="{{ 'accessibility.close' | t }}">{% render 'icon-close' %}</button>
  <div class="modal-content">
  <div class="modal-image{% if block.settings.video != blank %} autoplaying-video{% endif %}">
    {% if block.settings.video != blank %}
    {% assign video_id = 'vid-aso-' | append: block.id %}
    {{
      block.settings.video
      | video_tag:
        id: video_id,
        image_size: '1200x',
        autoplay: true,
        loop: true,
        controls: false,
        muted: true
    }}
    {% endif %}
    {% if block.settings.image != blank and block.settings.video == blank %}
    {{ block.settings.image | image_url: width: 1024 | image_tag: loading: 'lazy' }}
    {% endif %}
    {% if block.settings.image == blank and block.settings.video == blank %}
    {% render 'media-placeholder-lifestyle', style: 3 %}
    {% endif %}
  </div>
  <div class="modal-text">
  <h2>{{ block.settings.title }}</h2>
  <div class="modal-text-subtitle">{{ block.settings.sub_title }}</div>
  <div class="rte">{{ block.settings.description }}</div>
  <a href="{{ block.settings.link }}" target="_blank" rel="noopener,nofollow">{{ block.settings.handle }}</a>
       <div class="aso-product-links aso-product-card-ratio--{{ settings.card_img_aspect_ratio }}">
      
       {% if block.settings.product_one != blank %}
       <div class="aso-modal-product-card">
        <a href="{{ block.settings.product_one.url }}">
        <div class="aso-modal-product-card-image">
          {{ block.settings.product_one.featured_media | image_url: width: 360 | image_tag: loading: 'lazy' }}
        </div>
        <div class="aso-modal-product-card-text">
          <h3>{{ block.settings.product_one.title }}</h3>
          {% if section.settings.show_vendor %}{{ block.settings.product_one.vendor }}{% endif %}
          {% if section.settings.show_price %}
          {% render 'price', product: block.settings.product_one, price_class: '' %}
          {% endif %}
          {% if section.settings.show_links %}<div class="aso-modal-product-link-label">{{ section.settings.link_label }}</div>{% endif %}
        </div>
        </a>
       </div>
       {% endif %}

      {% if block.settings.product_two != blank %}
       <div class="aso-modal-product-card">
       <a href="{{ block.settings.product_two.url }}">
        <div class="aso-modal-product-card-image">
          {{ block.settings.product_two.featured_media | image_url: width: 360 | image_tag: loading: 'lazy' }}
        </div>
        <div class="aso-modal-product-card-text">
          <h3>{{ block.settings.product_two.title }}</h3>
          {% if section.settings.show_vendor %}{{ block.settings.product_two.vendor }}{% endif %}
          {% if section.settings.show_price %}
          {% render 'price', product: block.settings.product_two, price_class: '' %}
          {% endif %}
          {% if section.settings.show_links %}<div class="aso-modal-product-link-label">{{ section.settings.link_label }}</div>{% endif %}
        </div>         
       </a>
       </div>
       {% endif %}  
       {% if block.settings.product_three != blank %}
        <div class="aso-modal-product-card">
       <a href="{{ block.settings.product_three.url }}">
        <div class="aso-modal-product-card-image">
          {{ block.settings.product_three.featured_media | image_url: width: 360 | image_tag: loading: 'lazy' }}
        </div>
        <div class="aso-modal-product-card-text">
          <h3>{{ block.settings.product_three.title }}</h3>
          {% if section.settings.show_vendor %}{{ block.settings.product_three.vendor }}{% endif %}
          {% if section.settings.show_price %}
          {% render 'price', product: block.settings.product_three, price_class: '' %}
          {% endif %}
          {% if section.settings.show_links %}<div class="aso-modal-product-link-label">{{ section.settings.link_label }}</div>{% endif %}
        </div>         
       </a>
        </div>
       {% endif %}
     </div>  

  </div>
  </div>
</div>
<div class="aso-modal-bg bg-pane"></div>
</div>
{% endfor %}
  
</div>

{% if section.settings.bottom_border %}
<hr class="section-border section-border-color-{{ section.settings.border_color }} section-border-width-{{ section.settings.border_width }}">
{% endif %}
  
</div>

<script>
document.addEventListener("DOMContentLoaded", function() {

  var sectionId = '{{ section.id }}';
  var lastFocusedElement;

  // Get all modal buttons within the specified section
  var modalLinks = document.querySelectorAll(`#grid-${sectionId} .modal-opener-link`);

  // Add click event listener to each bio button
  modalLinks.forEach(function(button) {
    button.addEventListener('click', function(e) {
      var index = this.getAttribute('data-index');
      lastFocusedElement = this;
      var modal = document.getElementById(`${sectionId}-modal-${index}`);
      if (modal) {
        modal.classList.add('open');
        // Focus on the close button when the modal is opened
        var closeButton = modal.querySelector('.modal-close');
        if (closeButton) {
          closeButton.focus();
        }        
      }
    });
  });

  // Get all modal backgrounds
  var modalBgs = document.querySelectorAll('.aso-modal-bg');
  
  // Add click event listener to each modal background
  modalBgs.forEach(function(modalBg) {
    modalBg.addEventListener('click', function() {
      var modal = this.closest('.aso-modal');
      modal.classList.remove('open');
      // Return focus to the modal opener link
      if (lastFocusedElement) {
        lastFocusedElement.focus();
      }
    });
  });
  
  // Get all close buttons
  var closeButtons = document.querySelectorAll('.modal-close');

  // Add click event listener to each close button
  closeButtons.forEach(function(button) {
    button.addEventListener('click', function() {
      var modal = this.parentElement.parentElement;
      modal.classList.remove('open');
      // Return focus to the modal opener link
      if (lastFocusedElement) {
        lastFocusedElement.focus();
      }
    });
  });
});
</script>


