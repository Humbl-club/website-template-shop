{{ 'section-logo-wall.css' | asset_url | stylesheet_tag }}

{% assign numb_items = section.blocks.size %}

{% assign logo_render_size = 400 %}
{% if section.settings.logo_size < 155 and section.settings.logo_size_tablet < 155 and section.settings.logo_size_mobile < 155 %}
{% assign logo_render_size = 300 %}
{% endif %}
{% if section.settings.logo_size < 105 and section.settings.logo_size_tablet < 105 and section.settings.logo_size_mobile < 105 %}
{% assign logo_render_size = 200 %}
{% endif %}
{% if section.settings.logo_size < 85 and section.settings.logo_size_tablet < 85 and section.settings.logo_size_mobile < 85 %}
{% assign logo_render_size = 160 %}
{% endif %}

{% assign base_speed = section.settings.animation_speed %}

{% assign scale_factor = numb_items | divided_by: 10.0 %}
{% assign animation_adjusted_speed = base_speed | times: scale_factor %}

{% style %}

.section-{{ section.id }} .section-inner {
  padding-top: {{ section.settings.top_padding | times: 0.8 }}px;
  padding-bottom: {{ section.settings.bottom_padding | times: 0.8 }}px;
}
  
@media only screen and (min-width: 750px) {
  .section-{{ section.id }} .section-inner {
    padding-top: {{ section.settings.top_padding }}px;
    padding-bottom: {{ section.settings.bottom_padding }}px;
  }  
}  

.section-{{ section.id }} .logo-grid {
  column-gap: {{ section.settings.logo_spacing_mobile }}px;
  row-gap: {{ section.settings.logo_spacing_mobile }}px;
}

.section-{{ section.id }} .logo-wall-ticker-container {
  grid-template-columns: repeat({{ numb_items }},1fr);
  column-gap: 0;
  animation-duration: {{ animation_adjusted_speed | divided_by: 1 | at_least: 12 }}s; 
}

.logo-wall-ticker-container .logo {
  margin: 0 {{ section.settings.logo_spacing_mobile }}px;
}
  
.section-{{ section.id }} .logo--ratio-portrait img {
  width: auto;
  height: {{ section.settings.logo_size_mobile }}px; 
}

.section-{{ section.id }} .logo--ratio-landscape img {
  width: {{ section.settings.logo_size_mobile }}px;
  height: auto;
}

.section-{{ section.id }} .logo--ratio-square img {
  width: {{ section.settings.logo_size_mobile }}px;
  height: auto;
}

.section-{{ section.id }} .logo-grid .logo--ratio-portrait img {
  width: auto;
  height: {{ section.settings.logo_size_mobile }}px;  
}

.section-{{ section.id }} .logo-grid .logo--ratio-landscape img {
  width: {{ section.settings.logo_size_mobile }}px;  
  height: auto;
}

.section-{{ section.id }} .logo-grid .logo--ratio-square img {
  width: {{ section.settings.logo_size_mobile }}px;  
  height: auto;
}

.section-{{ section.id }} .logo-placeholder {
  width: {{ section.settings.logo_size_mobile }}px;
  height: {{ section.settings.logo_size_mobile | divided_by: 2 }}px;
}

{% if section.settings.grid_columns_mobile == '1' %}
.section-{{ section.id }} .logo-grid-image {
  min-width: 100%;
}
{% endif %}
  
{% if section.settings.grid_columns_mobile == '2' %}
.section-{{ section.id }} .logo-grid-image {
  min-width: calc(50% - {{ section.settings.logo_spacing_mobile }}px);
}
{% endif %}

{% if section.settings.grid_columns_mobile == '3' %}
.section-{{ section.id }} .logo-grid-image {
  min-width: calc(33% - {{ section.settings.logo_spacing_mobile }}px);
}
{% endif %}
  
@media only screen and (min-width: 750px) {
  .logo-wall-ticker-container .logo {
    margin: 0 {{ section.settings.logo_spacing_tablet }}px;
  }
  .section-{{ section.id }} .logo-placeholder {
    width: {{ section.settings.logo_size_tablet }}px;
    height: {{ section.settings.logo_size_tablet | divided_by: 2 }}px;
  }
  
  .section-{{ section.id }} .logo-grid {
    column-gap: {{ section.settings.logo_spacing_tablet }}px;
    row-gap: {{ section.settings.logo_spacing_tablet }}px;
  }
  
  .section-{{ section.id }} .logo-wall-ticker-container {
    animation-duration: {{ animation_adjusted_speed }}s;
    column-gap: 0;
  } 
  
  .section-{{ section.id }} .logo-grid .logo--ratio-portrait img {
    width: auto;
    height: {{ section.settings.logo_size_tablet }}px;  
  }
  
  .section-{{ section.id }} .logo-grid .logo--ratio-landscape img {
    width: {{ section.settings.logo_size_tablet }}px;  
    height: auto;
  }
  
  .section-{{ section.id }} .logo-grid .logo--ratio-square img {
    width: {{ section.settings.logo_size_tablet }}px;  
    height: auto;
  }
  {% if section.settings.grid_columns_tablet == 'auto' %}
  .section-{{ section.id }} .logo-grid-image {
    min-width: 40px;
  }
  {% endif %}
  {% unless section.settings.grid_columns_tablet == 'auto' %}
  .section-{{ section.id }} .logo-grid-image {
    min-width: calc({{ 100 | divided_by: section.settings.grid_columns_tablet }}% - {{ section.settings.logo_spacing_tablet }}px);
  }
  {% endunless %}
}

@media only screen and (min-width: 1025px) {
  .section-{{ section.id }} .logo-placeholder {
    width: {{ section.settings.logo_size }}px;
    height: {{ section.settings.logo_size | divided_by: 2 }}px;
  }
  .logo-wall-ticker-container .logo {
    margin: 0 {{ section.settings.logo_spacing }}px;
  }
  .section-{{ section.id }} .logo-grid .logo--ratio-portrait img {
    width: auto;
    height: {{ section.settings.logo_size }}px;  
  }
  
  .section-{{ section.id }} .logo-grid .logo--ratio-landscape img {
    width: {{ section.settings.logo_size }}px;  
    height: auto;
  }
  .section-{{ section.id }} .logo-grid .logo--ratio-square img {
    width: {{ section.settings.logo_size }}px;  
    height: auto;
  }
  {% if section.settings.grid_columns_desktop == 'auto' %}
  .section-{{ section.id }} .logo-grid-image {
    min-width: 40px;
  }
  {% endif %}
  {% unless section.settings.grid_columns_desktop == 'auto' %}
  .section-{{ section.id }} .logo-grid-image {
    min-width: calc({{ 100 | divided_by: section.settings.grid_columns_desktop }}% - {{ section.settings.logo_spacing }}px);
  }
  {% endunless %}  
}
  
{% endstyle %}

<div class="section-container logo-wall-section section-{{ section.id }} color-scheme-{{ section.settings.text_color }} color-scheme-{{ section.settings.bg_color }}">

{% if section.settings.top_border %}
<hr class="section-border section-border-color-{{ section.settings.border_color }} section-border-width-{{ section.settings.border_width }}">
{% endif %}
  
<div class="section-inner align-{{ section.settings.content_alignment }}">  
{% unless section.settings.section_label == blank and section.settings.heading == blank and section.settings.subheading == blank %}
<div class="section-header {{ section.settings.content_width }} section-header-std-padding">
{% unless section.settings.section_label == blank %}<div class="section-label">{{ section.settings.section_label }}</div>{% endunless %}
{% unless section.settings.heading == blank %}<h2 class="{{ section.settings.heading_size }}">{{ section.settings.heading }}</h2>{% endunless %}
{% unless section.settings.subheading == blank %}{{ section.settings.subheading }}{% endunless %}
</div>
{% endunless %}

{% if section.settings.grid_style == 'ticker' %}

<div class="logo-wall-ticker-wrapper">

<div class="logo-wall-ticker-container">
{% for block in section.blocks %}
<div class="logo logo-{{ section.settings.logo_size }} logo-{{ section.settings.logo_colour }}{% if block.settings.image.width < block.settings.image.height %} logo--ratio-portrait{% endif %}{% if block.settings.image.width > block.settings.image.height %} logo--ratio-landscape{% endif %}{% if block.settings.image.width == block.settings.image.height %} logo--ratio-square{% endif %}">
{% unless block.settings.link == blank %}<a href="{{ block.settings.link }}">{% endunless %}
{% if block.settings.image %}
{{ block.settings.image | image_url: width: logo_render_size | image_tag: loading: 'lazy' }}
{% else %}
{% render 'icon-logo-placeholder' %}
{% endif %}
{% unless block.settings.link == blank %}</a>{% endunless %}
</div>
{% endfor %}
</div>
  
<div class="logo-wall-ticker-container">
{% for block in section.blocks %}
<div class="logo logo-{{ section.settings.logo_size }} logo-{{ section.settings.logo_colour }}{% if block.settings.image.width < block.settings.image.height %} logo--ratio-portrait{% endif %}{% if block.settings.image.width > block.settings.image.height %} logo--ratio-landscape{% endif %}{% if block.settings.image.width == block.settings.image.height %} logo--ratio-square{% endif %}">
{% unless block.settings.link == blank %}<a href="{{ block.settings.link }}">{% endunless %}
{% if block.settings.image %}
{{ block.settings.image | image_url: width: logo_render_size | image_tag: loading: 'lazy' }}
{% else %}
{% render 'icon-logo-placeholder' %}
{% endif %}
{% unless block.settings.link == blank %}</a>{% endunless %}
</div>
{% endfor %}
</div>

{% if section.blocks.size < 9 %}
  
<div class="logo-wall-ticker-container">
{% for block in section.blocks %}
<div class="logo logo-{{ section.settings.logo_size }} logo-{{ section.settings.logo_colour }}{% if block.settings.image.width < block.settings.image.height %} logo--ratio-portrait{% endif %}{% if block.settings.image.width > block.settings.image.height %} logo--ratio-landscape{% endif %}{% if block.settings.image.width == block.settings.image.height %} logo--ratio-square{% endif %}">
{% unless block.settings.link == blank %}<a href="{{ block.settings.link }}">{% endunless %}
{% if block.settings.image %}
{{ block.settings.image | image_url: width: logo_render_size | image_tag: loading: 'lazy' }}
{% else %}
{% render 'icon-logo-placeholder' %}
{% endif %}
{% unless block.settings.link == blank %}</a>{% endunless %}
</div>
{% endfor %}
</div>

<div class="logo-wall-ticker-container">
{% for block in section.blocks %}
<div class="logo logo-{{ section.settings.logo_size }} logo-{{ section.settings.logo_colour }}{% if block.settings.image.width < block.settings.image.height %} logo--ratio-portrait{% endif %}{% if block.settings.image.width > block.settings.image.height %} logo--ratio-landscape{% endif %}{% if block.settings.image.width == block.settings.image.height %} logo--ratio-square{% endif %}">
{% unless block.settings.link == blank %}<a href="{{ block.settings.link }}">{% endunless %}
{% if block.settings.image %}
{{ block.settings.image | image_url: width: logo_render_size | image_tag: loading: 'lazy' }}
{% else %}
{% render 'icon-logo-placeholder' %}
{% endif %}
{% unless block.settings.link == blank %}</a>{% endunless %}
</div>
{% endfor %}
</div> 

<div class="logo-wall-ticker-container">
{% for block in section.blocks %}
<div class="logo logo-{{ section.settings.logo_size }} logo-{{ section.settings.logo_colour }}{% if block.settings.image.width < block.settings.image.height %} logo--ratio-portrait{% endif %}{% if block.settings.image.width > block.settings.image.height %} logo--ratio-landscape{% endif %}{% if block.settings.image.width == block.settings.image.height %} logo--ratio-square{% endif %}">
{% unless block.settings.link == blank %}<a href="{{ block.settings.link }}">{% endunless %}
{% if block.settings.image %}
{{ block.settings.image | image_url: width: logo_render_size | image_tag: loading: 'lazy' }}
{% else %}
{% render 'icon-logo-placeholder' %}
{% endif %}
{% unless block.settings.link == blank %}</a>{% endunless %}
</div>
{% endfor %}
</div> 
  
{% endif %}
  
</div>

{% else %}
  
<div class="logo-grid {{ section.settings.content_width }} logo-grid-{{ section.settings.logo_colour }}">
{% for block in section.blocks %}
{% if block.settings.image %}
<div class="logo-grid-image{% if block.settings.image.width < block.settings.image.height %} logo--ratio-portrait{% endif %}{% if block.settings.image.width > block.settings.image.height %} logo--ratio-landscape{% endif %}{% if block.settings.image.width == block.settings.image.height %} logo--ratio-square{% endif %}" {{ block.shopify_attributes }}>{% unless block.settings.link == blank %}<a href="{{ block.settings.link }}" rel="noopener">{% endunless %}{{ block.settings.image | image_url: width: logo_render_size | image_tag: loading: 'lazy' }}{% unless block.settings.link == blank %}</a>{% endunless %}</div>
{% else %}
<div class="logo-grid-image{% if block.settings.image.width < block.settings.image.height %} logo--ratio-portrait{% endif %}{% if block.settings.image.width > block.settings.image.height %} logo--ratio-landscape{% endif %}{% if block.settings.image.width == block.settings.image.height %} logo--ratio-square{% endif %}" {{ block.shopify_attributes }}>
  {% render 'icon-logo-placeholder' %}
</div>
{% endif %}
{% endfor %}  
</div>

{% endif %}
  
{% if section.settings.btn_enable %}
<div class="section-footer button-color-scheme--{{ section.settings.btn_color_scheme }}">
    {% if section.settings.btn_style == 'solid' %}
	<a href="{{ section.settings.btn_link }}" role="link" class="button button--solid">{{ section.settings.btn_txt }}</a>
    {% endif %}
    {% if section.settings.btn_style == 'outline' %}
	<a href="{{ section.settings.btn_link }}" role="link" class="button button--outline">{{ section.settings.btn_txt }}</a>
    {% endif %}
    {% if section.settings.btn_style == 'link' %}
	<a href="{{ section.settings.btn_link }}" role="link" class="link--underlined">{{ section.settings.btn_txt }}</a>
    {% endif %}
</div>
{% endif %}
  
</div>

{% if section.settings.bottom_border %}
<hr class="section-border section-border-color-{{ section.settings.border_color }} section-border-width-{{ section.settings.border_width }}">
{% endif %}
  
</div>

{% schema %}
  {
    "name": "t:sections.logo_wall.name",
  	"tag": "section",
  	"class": "section",    
    "disabled_on": {
      "groups": ["header", "footer"],
      "templates": ["cart"]
    },
    "max_blocks": 40,
    "settings": [
    {
      "type": "text",
      "id": "section_label",
      "label": "t:sections.all.caption"
	},
    {
      "type": "text",
      "id": "heading",
      "label": "t:sections.all.heading"
	},
    {
      "type": "select",
      "id": "heading_size",
      "label": "t:sections.all.heading_size.label",
      "options": [
        {
          "value": "h3",
