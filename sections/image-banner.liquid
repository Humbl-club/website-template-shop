{{ 'section-image-banner.css' | asset_url | stylesheet_tag }}

{% assign fetch_priority = 'auto' %}
{% assign image_loading = 'lazy' %}
{% if section.index == 1 %}
{% assign fetch_priority = 'high' %}
{% assign image_loading = 'eager' %}
{% endif %}

{% assign heading_html_tag = 'h2' %}

{% if section.index == 1 %}
{% assign heading_html_tag = 'h1' %}  
{% endif %}

{%- style -%}

.section-{{ section.id }} {
  background: {{ section.settings.bg_color }};
}

.section-{{ section.id }} .section-inner {
  padding-top: {{ section.settings.top_padding }}px;
  padding-bottom: {{ section.settings.bottom_padding }}px;
}

{%- if section.settings.overlay_level < 10 -%}
.section-{{ section.id }} .banner-overlay {
  background: rgba(0,0,0,0.05);
}
{%- else -%}
.section-{{ section.id }} .banner-overlay {
  background: rgba(0,0,0,0.{{ section.settings.overlay_level }});
}
{%- endif -%}

.section-{{ section.id }}.color-scheme-fg-white .button-color-scheme--match-section .button--solid {
  background: #ffffff;
  color: var(--color-foreground);
}

.section-{{ section.id }}.color-scheme-fg-white .button-color-scheme--match-section .button--outline {
  color: #ffffff;
}

.section-{{ section.id }} .banner-primary-image {
  padding: {{ section.settings.image_padding | divided_by: 1.4 }}px;
}

.section-{{ section.id }} .banner-secondary-image {
  padding: {{ section.settings.secondary_image_padding | divided_by: 1.4 }}px;
}

.section-{{ section.id }} .banner-primary-image img, .section-{{ section.id }} .banner-primary-image .luxe-product-placeholder-wrapper {
  filter: blur({{ section.settings.image_blur }}px);
}

.section-{{ section.id }} .banner-secondary-image img {
  filter: blur({{ section.settings.secondary_image_blur }}px); 
}

.section-{{ section.id }} .banner-full-bleed-text > div {
  padding: {{ section.settings.text_vertical_spacing }}px 0;
}

@media only screen and (min-width: 900px) {
  .section-{{ section.id }} .banner-primary-image {
    padding: {{ section.settings.image_padding }}px;
  }
  .section-{{ section.id }} .banner-secondary-image {
    padding: {{ section.settings.secondary_image_padding }}px;
  }  
}

{%- endstyle -%}

<div class="section-container{% if section.index == 1 %} landing-screen-section{% endif %} section-image-banner banner-full-bleed section-{{ section.id }} {{ section.settings.banner_size }} color-scheme-{{ section.settings.text_color }}">

<div class="section-inner banner-text-position--{{ section.settings.txt_position }}">
{% if section.settings.banner_full_link != blank %}
<a class="image-banner-full-banner-link" href="{{ section.settings.banner_full_link }}"></a>
{% endif %}
<div class="banner-full-bleed-text banner-text-width--{{ section.settings.content_width | escape }} align--{{ section.settings.txt_alignment }}{% if section.settings.fade_in_text %} banner-text-fade-in{% endif %}">

{% for block in section.blocks %}
{% case block.type %}

{% when 'logo' %}
{% assign logo_width = block.settings.logo_width_desktop | times: 2 %}

{% if block.settings.logo_full_width %}
{% assign logo_width = 2400 %}
{% endif %}
  
<style>
  .banner-logo {
    width: {{ block.settings.logo_width_mobile }}px;
    max-width: 100%;
  }
  @media only screen and (min-width: 1025px) {
    .banner-logo {
      width: {{ block.settings.logo_width_desktop }}px;
      max-width: 100%;
    }    
  }
  {% if block.settings.logo_full_width %}
  .banner-logo {
    width: 100% !important;
  }
  {% endif %}
</style>
  
<div class="banner-logo" {{ block.shopify_attributes }}>
{% if block.settings.logo %}{{ block.settings.logo | image_url: width: logo_width | image_tag }}{% endif %}
</div>
  
{% when 'caption' %}
<div class="section-label" {{ block.shopify_attributes }}>
{{ block.settings.caption }}
</div>
  
{% when 'heading' %}
<div class="image-banner-heading align-{{ block.settings.txt_alignment }}" {{ block.shopify_attributes }}>
{% unless block.settings.txt_alignment == 'asymmetric' and block.settings.heading contains '/' %}
<{{ heading_html_tag }} class="{{ block.settings.heading_size }}">{{ block.settings.heading }}</{{ heading_html_tag }}>
{% endunless %}

{% if block.settings.txt_alignment == 'asymmetric' and block.settings.heading contains '/' %}
{% assign words = block.settings.heading | split: ' / ' %}
<{{ heading_html_tag }} class="{{ block.settings.heading_size }} asymmetric-heading-{{ words.size }}-words">
  {% for word in words %}
    <div class="image-banner-word">{{ word }}</div>
  {% endfor %}
</{{ heading_html_tag }}>
{% endif %}
  
</div>
  
{% when 'text' %}
<div class="{{ block.settings.text_size }}" {{ block.shopify_attributes }}>
{{ block.settings.text }}
</div>  

{% when 'buttons' %}
<style>
  .section-{{ section.id }} .image-banner-buttons-inline {
    padding-top: {{ block.settings.padding_top }}px !important;
    padding-bottom: {{ block.settings.padding_bottom }}px !important;
  }
</style>
{% assign button_one_link = block.settings.btn_one_link %}
{% assign button_two_link = block.settings.btn_two_link %}

{% if section.settings.banner_full_link != blank %}
{% assign button_one_link = '#' %}
{% assign button_two_link = '#' %}
{% endif %}
  
{% assign color_scheme = block.settings.btn_color_scheme %}
{% assign button_layout = block.settings.btn_layout %}
  
<div class="image-banner-buttons-inline{% if block.settings.btn_one_show and block.settings.btn_two_show %} buttons-layout-{{ button_layout }}{% endif %} button-color-scheme--{{ color_scheme }}" {{ block.shopify_attributes }}> 
{% if block.settings.btn_one_show %}
<a href="{{ button_one_link }}" class="{% unless block.settings.btn_one_style == 'link--underlined' %}button{% endunless %} {{ block.settings.btn_one_style }}">{{ block.settings.btn_one_txt }}</a>  
{% endif %}
{% if block.settings.btn_two_show %}
<a href="{{ button_two_link }}" class="{% unless block.settings.btn_two_style == 'link--underlined' %}button{% endunless %} {{ block.settings.btn_two_style }}">{{ block.settings.btn_two_txt }}</a> 
{% endif %}
</div>
{% endcase %}
{% endfor %}

</div>

{% assign primary_image_class = 'std' %}  
{% assign secondary_image_class = 'std' %}

{% if section.settings.primary_image_mobile_show == false %}
{% assign primary_image_class = 'image-mob-hide' %}
{% endif %}

{% if section.settings.secondary_image_mobile_show == false %}
{% assign secondary_image_class = 'image-mob-hide' %}
{% endif %}
  
<div class="bg-pane banner-overlay"></div>
{% if section.settings.scroll_fade_effect %}<div class="bg-pane banner-overlay-fader"></div>{% endif %}
<div class="bg-pane banner-bg-image">

{% if section.settings.image %}
<div class="banner-primary-image {{ section.settings.primary_image_size }} image-animation-{{ section.settings.main_image_animation }}">
{% if section.settings.primary_image_size == 'dt-full-bleed' %}
{{ section.settings.image | image_url: width: 2800 | image_tag: class: primary_image_class, fetchpriority: fetch_priority, loading: image_loading }}
{% else %}
{{ section.settings.image | image_url: width: 1800 | image_tag: class: primary_image_class, fetchpriority: fetch_priority, loading: image_loading }}
{% endif %}
</div>
{% endif %}

{% if section.settings.image_secondary %}
<div class="banner-secondary-image {{ section.settings.secondary_image_size }} image-animation-{{ section.settings.secondary_image_animation }}">
{% if section.settings.secondary_image_size == 'dt-full-bleed' %}
{{ section.settings.image_secondary | image_url: width: 2800 | image_tag: class: secondary_image_class, fetchpriority: fetch_priority, loading: image_loading }}
{% else %}
{{ section.settings.image_secondary | image_url: width: 1800 | image_tag: class: secondary_image_class, fetchpriority: fetch_priority, loading: image_loading }}
{% endif %}  
</div>
{% endif %}

{% if section.settings.image == blank and section.settings.image_secondary == blank %}
<div class="banner-primary-image {{ section.settings.primary_image_size }}">
{% render 'media-placeholder-lifestyle', style: 11 %}
</div>
{% endif %}

</div>


</div>

</div>

{% if section.settings.scroll_fade_effect %}
<script>
  document.addEventListener("DOMContentLoaded", function() {
    const sectionId = "{{ section.id }}";
    const sectionElement = document.querySelector(`.section-${sectionId}`);
    const bgPane = sectionElement.querySelector(".banner-overlay-fader");
    const headingDiv = sectionElement.querySelector(".banner-full-bleed-text");
    const headingHeight = headingDiv.offsetHeight;

    window.addEventListener("scroll", function() {
      const scrollPosition = window.scrollY;
      const maxScroll = window.innerHeight;

      const opacity = Math.min(scrollPosition / maxScroll, 1);
      
      // Set the background color with the calculated opacity
      //bgPane.style.backgroundColor = `rgba(0, 0, 0, ${opacity})`;

      // Adjust 'top' position of heading based on scroll position
      const topOffset = -scrollPosition * 0.25;
      headingDiv.style.top = `${topOffset}px`;

      // Fade the text opacity as scroll happens
      const textOpacity = 1 - (scrollPosition / maxScroll); // Invert opacity
      headingDiv.style.opacity = textOpacity;
    });
  });
</script>
{% endif %}
  
